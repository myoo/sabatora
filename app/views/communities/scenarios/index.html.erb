<h1>Listing communities_scenarios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @communities_scenarios.each do |communities_scenario| %>
      <tr>
        <td><%= link_to 'Show', communities_scenario %></td>
        <td><%= link_to 'Edit', edit_communities_scenario_path(communities_scenario) %></td>
        <td><%= link_to 'Destroy', communities_scenario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scenario', new_communities_scenario_path %>
