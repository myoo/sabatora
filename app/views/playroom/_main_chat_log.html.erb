<div id="session_log" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myModalLabel">チャットログダウンロード</h4>
            </div>
            <div class="modal-body">
                <%= render "logs/download_form" %>
            </div>

        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>